URxvt.intensityStyles:	false
URxvt.background:       [70]#000000
URxvt.depth:		32
URxvt.font: 		xft:monospace:size=11
URxvt.scrollBar:	false
URxvt.scrollstyle:      rxvt
URxvt.cursorColor:	white

!! Extensions
URxvt.perl-ext-common:	default,matcher,url-select,keyboard-select

!! URLs
URxvt.keysym.C-U:       perl:url-select:select_next
URxvt.url-launcher:     /usr/bin/firefox -new-tab
URxvt.underlineURLs:    True
!URxvt.colorUL:		#4682B4
URxvt.matcher.button:   1

URxvt.keysym.Shift-Control-V:   eval:paste_clipboard
URxvt.keysym.Shift-Control-c:   eval:selection_to_clipboard
